{% block body %}
<p>{{ message }}</p>
<form action="">
    <input type="text" name="term" value="{{ term|striptags }}">
    <input type="submit" value="Find user">
</form>
<a href="{{url_for('adduser')}}">Регистрация</a>

<ul>
    {% for user in users %}
        <li>{{ user['name']}} - {{ user['email']}}</li>
    {% endfor %}
</ul>
{% endblock %}